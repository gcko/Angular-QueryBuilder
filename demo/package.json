{
  "name": "demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --raw \"gulp copyLibWatch\" \"ng serve\"",
    "build": "npm run build:aot && npm run build:jit && npm run copy-editor",
    "build:aot": "ng build --configuration production",
    "build:jit": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "copy-editor": "copyfiles \"./editor/**/*\" dist",
    "copy-lib": "npm install ../dist/angular2-query-builder",
    "prebuild:aot": "npm run copy-lib",
    "prebuild:jit": "npm run copy-lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "~12.2.13",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/material": "~12.2.13",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "angular2-query-builder": "file:../dist/angular2-query-builder",
    "core-js": "^2.6.5",
    "rxjs": "~6.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.9",
